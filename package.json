{
  "name": "@prismlabsdev/prism-chat-lib-js",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/PrismLabsDev/prism-chat-lib-js",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "npm run build",
    "test": "jest --verbose",
    "clean": "rm -rf dist/ *.tgz"
  },
  "keywords": [
    "PrismLabs",
    "PrismChat",
    "cryptography",
    "libsodium"
  ],
  "author": "PrismLabsDev",
  "license": "MIT",
  "description": "Prism Chat encryption library, e2e, anonymous & decentralized messaging.",
  "bugs": {
    "url": "https://github.com/PrismLabsDev/prism-chat-lib-js/issues"
  },
  "homepage": "https://PrismLabs.dev/",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/libsodium-wrappers-sumo": "^0.7.8",
    "@types/node": "^24.8.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "libsodium-wrappers-sumo": "^0.7.15"
  }
}
